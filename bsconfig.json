{
  "name": "rescript-test-project",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": false
  },
  "suffix": ".bs.js",
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "bs-dependencies": [
    "@rescript/core",
    "rescript-json"
  ],
  "reanalyze": {
    "analysis": ["dce", "exception"]
  },
  "gentypeconfig": {
    "language": "typescript",
    "generatedFileExtension": ".gen.tsx",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}